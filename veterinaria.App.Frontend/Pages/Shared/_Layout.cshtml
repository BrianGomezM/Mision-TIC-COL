<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HomePetCare</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- <link rel="manifest" href="site.webmanifest"> -->
    <link rel="shortcut icon" type="image/x-icon" href="~/img/favicon.png">
    <!-- Place favicon.ico in the root directory -->

    <!-- CSS here -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/css/magnific-popup.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/themify-icons.css">
    <link rel="stylesheet" href="~/css/nice-select.css">
    <link rel="stylesheet" href="~/css/flaticon.css">
    <link rel="stylesheet" href="~/css/gijgo.css">
    <link rel="stylesheet" href="~/css/animate.css">
    <link rel="stylesheet" href="~/css/slicknav.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/custom.css">
    <!-- <link rel="stylesheet" href="css/responsive.css"> -->
</head>
<body>
   <!-- header_start  -->
     <header>
        <div class="header-area ">
            <div class="header-top_area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-5 col-md-12">
                            <div class="short_contact_list">
                                <ul>
                                    <li><a href="#">3186743218</a></li>
                                    <li><a href="#">Lunes - Sabado 8:00 a.m - 7:00 p.m</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-7 col-md-12 ">
                            <div class="social_media_links">
                                <a href="#">
                                    <i class="fa fa-facebook">HotPetCare</i>
                                </a>
                                <a href="#">
                                    <i class="fa fa-instagram">HotPetCare</i>
                                </a>
                                <a href="#">
                                    <i class="fa fa-google-plus">HotPetCare@Gmail.com</i>
                                </a>
                                <a href="#" >
                                    <a asp-page="/IniciarSesion/IniciarSesion"><i class="fa fa-window-close"> Cerrar Sesiòn</i></a>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="sticky-header" class="main-header-area">
                <div class="container" >
                    <div  class="row align-items-center ">
                        <div class="col-xl-2 col-lg-2">
                            <div class="logo">
                                <a asp-page="/Inicio/Inicio">                         
                                    <img src="../../img/logo2.png" alt="" >
                                </a>
                            </div>
                        </div>
                        <div class="col-xl-10 col-lg-12">
                            <div class="main-menu  d-none d-lg-block">
                                <nav>
                                    <ul id="navigation">
                                        <li><a asp-page="/Inicio/Inicio">Inicio</a></li>
                                        <li><a asp-page="/Somos/Somos">¿Quiénes somos?</a></li>                                 
                                        <li><a asp-page="#">Gatos<i class="ti-angle-down"></i></a>
                                            <ul class="submenu">
                                                <li><a asp-page="/Gato/CrearGato/CrearGato">Registar Gato</a></li>
                                                <li><a asp-page="/Gato/ListarGato/ListarGato">Listar Gatos</a></li>
                                            </ul>
                                        </li>
                                        <li><a asp-page="#">Personas<i class="ti-angle-down"></i></a>
                                            <ul class="submenu">
                                                <li><a asp-page="/Persona/RegistrarPersona/RegistrarPersona">Registrar Persona</a></li>
                                                <li><a asp-page="/Persona/ListarPersona/ListarPersona">Listar Persona</a></li>
                                            </ul>
                                        </li>                                
                                        <li><a asp-page="#">Historia Clinica<i class="ti-angle-down"></i></a>
                                            <ul class="submenu">
                                                <li><a asp-page="/HistoriaClinica/RegistrarHistoria/RegistrarHistoria">Registrar Historia Clinica</a></li>
                                                <li><a asp-page="/HistoriaClinica/ListarHistoria/ListarHistoria">Listar Historia Clinica</a></li>
                                            </ul>
                                        </li>
                                        <li><a asp-page="/Agenda/Agenda">Agenda</a></li>
                                        <li><a asp-page="/Galeria/Galeria">Galeria</a></li>                                        
                                    </ul>
                                </nav>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>
                </div>
        </div>
    </header>
    <!-- header_start  -->
    <div class="content">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!-- service_area_end -->


    <!-- footer_start  -->
    <footer class="footer">
        <div class="header-area ">
            <div class="header-top_area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 col-md-8">
                            <div class="short_contact_list">
                                <ul>
                                    <li><a href="#">3186743218</a></li>
                                    <li><a href="#">Lunes - Sabado 8:00 a.m - 7:00 p.m</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-4 ">
                            <div class="social_media_links">
                                <a href="#">
                                    <i class="fa fa-facebook"> HotPetCare</i>
                                </a>
                                <a href="#">
                                    <i class="fa fa-instagram"> HotPetCare</i>
                                </a>
                                <a href="#">
                                    <i class="fa fa-google-plus"> HotPetCare@Gmail.com</i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </footer>
    <!-- footer_end  -->
   

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- footer_end  -->
    
        <!-- JS here -->
        <script src="~/js/vendor/modernizr-3.5.0.min.js"></script>
        <script src="~/js/vendor/jquery-1.12.4.min.js"></script>
        <script src="~/js/popper.min.js"></script>
        <script src="~/js/bootstrap.min.js"></script>
        <script src="~/js/owl.carousel.min.js"></script>
        <script src="~/js/isotope.pkgd.min.js"></script>
        <script src="~/js/ajax-form.js"></script>
        <script src="~/js/waypoints.min.js"></script>
        <script src="~/js/jquery.counterup.min.js"></script>
        <script src="~/js/imagesloaded.pkgd.min.js"></script>
        <script src="~/js/scrollIt.js"></script>
        <script src="~/js/jquery.scrollUp.min.js"></script>
        <script src="~/js/wow.min.js"></script>
        <script src="~/js/nice-select.min.js"></script>
        <script src="~/js/jquery.slicknav.min.js"></script>
        <script src="~/js/jquery.magnific-popup.min.js"></script>
        <script src="~/js/plugins.js"></script>
        <script src="~/js/gijgo.min.js"></script>
    
        <!--contact js-->
        <script src="~/js/contact.js"></script>
        <script src="~/js/jquery.ajaxchimp.min.js"></script>
        <script src="~/js/jquery.form.js"></script>
        <script src="~/js/jquery.validate.min.js"></script>
        <script src="~/js/mail-script.js"></script>
    
        <script src="~/js/main.js"></script>
        <script>
        $('#datepicker').datepicker({
            iconsLibrary: 'fontawesome',
            disableDaysOfWeek: [0, 0],
        //     icons: {
        //      rightIcon: '<span class="fa fa-caret-down"></span>'
        //  }
        });
        $('#datepicker2').datepicker({
            iconsLibrary: 'fontawesome',
            icons: {
             rightIcon: '<span class="fa fa-caret-down"></span>'
         }
        });
        var timepicker = $('#timepicker').timepicker({
         format: 'HH.MM'
     });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
